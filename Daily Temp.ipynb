    "def dailyTem(T):\n",
    "    val=[0]*len(T)\n",
    "    stack=[]\n",
    "\n",
    "    for i in range(len(T)):\n",
    "        while stack and T[i]>T[stack[-1]]:\n",
    "            index=stack.pop()\n",
    "            val[index]=i-index\n",
    "        stack.append(i)\n",
    "\n",
    "    return val\n",
    "T = [73, 74, 75, 71, 69, 72, 76, 73]\n",
    "print(dailyTem(T))"
   ]
