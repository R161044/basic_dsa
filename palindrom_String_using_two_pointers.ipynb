    "def pal_or_not(arr):\n",
    "    left=0\n",
    "    right=len(arr)-1\n",
    "    pal=True\n",
    "\n",
    "    while left<right:\n",
    "        if arr[left]!=arr[right]:\n",
    "            pal=False\n",
    "        left+=1\n",
    "        right-=1\n",
    "            \n",
    "    return pal\n",
    "\n",
    "\n",
    "arr='madam'\n",
    "print(pal_or_not(arr))"
   ]
